{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "images/128.png",
      "default_popup": "popup.html",
      "default_title": "確認する"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "0時から早朝6時の間にchromeを起動していると、30分毎にクソネミレベル（徹夜時間）が増えてゆきます。",
   "icons": {
      "128": "images/128.png",
      "16": "images/128.png",
      "48": "images/128.png"
   },
   "manifest_version": 2,
   "name": "徹夜の記録",
   "permissions": ["notifications"],
   "version": "0.0.8"
}
